{
  "expo": {
    "name": "domu-app",
    "slug": "domu-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/domu-logo.png",
    "scheme": "domuapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.neider08urbano.domuapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/domu-logo.png",
        "backgroundImage": "./assets/images/domu-logo.png",
        "monochromeImage": "./assets/images/domu-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.neider08urbano.domuapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/domu-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/domu-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "50d8bcb8-ac5b-4965-a59f-9da90965dcd0"
      }
    }
  }
}
